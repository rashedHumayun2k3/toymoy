{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "toy-moy-client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/toy-moy-client",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              "src/assets",
              "src/app/assets"
            ],
            "styles": [
              "src/assets/css/material-design-iconic-font.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/fontawesome-stars.css",
              "src/assets/css/meanmenu.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/slick.css",
              "src/assets/css/animate.css",
              "src/assets/css/jquery-ui.min.css",
              "src/assets/css/venobox.css",
              "src/assets/css/nice-select.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/helper.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css"
            
            ],
          
            "scripts": [
              "src/assets/js/vendor/jquery-1.12.4.min.js",
              "src/assets/js/vendor/popper.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/ajax-mail.js",
              "src/assets/js/jquery.meanmenu.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/js/imagesloaded.pkgd.min.js",
              "src/assets/js/jquery.mixitup.min.js",
              "src/assets/js/jquery.countdown.min.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/jquery.barrating.min.js",
              "src/assets/js/jquery-ui.min.js",
              "src/assets/js/venobox.min.js",
              "src/assets/js/jquery.nice-select.min.js",
              "src/assets/js/scrollUp.min.js",
              "src/assets/js/main.js" ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "1.5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2MB",
                  "maximumError": "1.5MB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "toy-moy-client:build:production"
            },
            "development": {
              "buildTarget": "toy-moy-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }

    }
  },
  "cli": {
    "analytics": "f03cff3a-d93b-46f7-a394-51db29b45963"
  }
}
